    <div class="container" style="margin-top: 30px">
        <div class="row">
            <div class="col-12">
                <div class="card my-2">
                    <div class="card-header">Input Data</div>
                    <div class="card-body">
                        <form id="submitForm" method="POST">
                            <div class="form-group">
                                <label for="comment">CSV:</label>
                                <textarea class="form-control" rows="5" id="csv_string"></textarea>
                            </div>
                            <button class="btn btn-primary" type="submit">Nháº­p</button>
                        </form>
                    </div>
                </div>

                <div class="card my-3">
                    <div class="card-header">Result</div>
                    <div class="card-body">
                        <ul class="nav nav-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" href="#tabs-1" role="tab">Result of
                                    Data</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#tabs-2" role="tab">Data After Remove Null
                                    Row And Auto Fill
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#tabs-3" role="tab">Data After
                                    Normalization</a>
                            </li>
                        </ul>
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div class="tab-pane active" id="tabs-1" role="tabpanel">
                                <table id="result_table" class="table table-striped table-bordered dt-responsive nowrap"
                                    style="width: 100%"></table>
                            </div>
                            <div class="tab-pane" id="tabs-2" role="tabpanel">
                                <table id="removenull_result_table"
                                    class="table table-striped table-bordered dt-responsive nowrap" style="width: 100%">
                                </table>
                            </div>
                            <div class="tab-pane" id="tabs-3" role="tabpanel">
                                <table id="normalization_result_table"
                                    class="table table-striped table-bordered dt-responsive nowrap" style="width: 100%">
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
